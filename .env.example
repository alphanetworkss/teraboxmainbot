# Environment Configuration
# Copy this file to .env and fill in your values

# ===== TELEGRAM BOT CONFIGURATION =====
# Main bot token (handles user interactions)
MAIN_BOT_TOKEN=your_main_bot_token_here

# Telegram API credentials (for Pyrogram upload clients)
API_ID=123
API_HASH=xxx

# Upload bot tokens (comma-separated, 3-5 recommended)
# These bots will be used for uploading videos (round-robin)
UPLOAD_BOT_TOKENS=bot_token_1,bot_token_2,bot_token_3

# Log channel ID (where videos are first uploaded)
# Format: -100xxxxxxxxxx
LOG_CHANNEL_ID=-1001234567890

# Force subscribe channel ID (optional, set to 0 to disable)
# Users must join this channel to use the bot
# Format: Can be channel username (@Thestarbots) or channel ID (-100xxxxxxxxxx)
# Examples:
#   FORCE_SUBSCRIBE_CHANNEL_ID=@Thestarbots
#   FORCE_SUBSCRIBE_CHANNEL_ID=-1001234567890
#   FORCE_SUBSCRIBE_CHANNEL_ID=0  (disabled)
FORCE_SUBSCRIBE_CHANNEL_ID=0

# ===== DATABASE CONFIGURATION =====
# MongoDB connection string
MONGODB_URI=mongodb://localhost:27017
MONGODB_DB_NAME=terabox_bot

# ===== REDIS CONFIGURATION =====
# Redis connection
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# ===== TERABOX API CONFIGURATION =====
# Starbots TeraBox API endpoint
TERABOX_API_URL=https://api.starbots.in/api/terabox

# ===== WORKER CONFIGURATION =====
# Maximum concurrent downloads per worker
MAX_CONCURRENT_DOWNLOADS=3

# Download directory (temporary storage)
DOWNLOAD_DIR=./downloads

# ===== LOGGING =====
LOG_LEVEL=INFO
LOG_FILE_MAX_SIZE=100MB
